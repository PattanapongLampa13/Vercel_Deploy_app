{
  "builds": [
    {
      "src": "vercel_deploy/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/statics/(.*)",
      "dest": "/staticfiles_build/statics/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_deploy/wsgi.py"
    }
  ]
}
